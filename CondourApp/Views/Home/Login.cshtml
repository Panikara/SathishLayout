@{
    Layout = null;
}
<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8" />
    <title>Sathish Layout</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />
    <meta content="A fully featured admin theme which can be used to build CRM, CMS, etc." name="description" />
    <meta content="Coderthemes" name="author" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <!-- App favicon -->
    <link rel="shortcut icon" href="assets/images/favicon.ico">
    <!-- App css -->
    <link href="~/Content/App1/assets/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/App1/assets/css/icons.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/App1/assets/css/style.css" rel="stylesheet" type="text/css" />
    <script src="~/Content/App1/assets/js/modernizr.min.js"></script>
</head>
@using (Html.BeginForm("Login", "Home", FormMethod.Post, new { id = "LoginForm", data_toggle = "validator" }))
{
<body>
    @Html.AntiForgeryToken()
    <input type="hidden" name="ReturnUrl" value="@Request.QueryString["ReturnUrl"]" />
    <div class="account-pages"></div>
    <div class="clearfix"></div>
    <div class="wrapper-page">
        <div class="text-center">
            <a href="index.html" class="logo"><span>Sathish<span> MACH Society</span></span></a>
            @*<h5 class="text-muted mt-0 font-600">Application Management</h5>*@
        </div>
        <div class="m-t-40 card-box">
            <div class="text-center">
                <h4 class="text-uppercase font-bold mb-0">Sign In</h4>
            </div>
            <div class="p-20">
                @using (Html.BeginForm("Login", "Home", FormMethod.Post, new { enctype = "multipart/form-data" }))
                {
                    @*<form class="form-horizontal m-t-20" action="https://coderthemes.com/adminto/horizontal/index.html">*@
                        <div class="form-group">
                            <div class="col-xs-12">
                                <input class="form-control" type="text" required="" placeholder="Username" name="userName">
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-xs-12">
                                <input class="form-control" type="password" required="" placeholder="Password" name="pwd">
                            </div>
                        </div>
                        <div class="form-group ">
                            <div class="col-xs-12">
                                <div class="checkbox checkbox-custom">
                                    <input id="checkbox-signup" type="checkbox">
                                    <label for="checkbox-signup">
                                        Remember me
                                    </label>
                                </div>
                            </div>
                        </div>
                        <div class="form-group text-center m-t-30">
                            <div class="col-xs-12">
                                <button class="btn btn-primary  waves-effect waves-light" type="submit">Log In</button>
                            </div>
                        </div>
                        <div style="color:red">@ViewBag.status</div>
                    @*</form>*@
                }


            </div>
        </div>
       <div>

        
       </div>

        <div style="color:forestgreen;text-align:center">
            @Html.ActionLink("Create New Account", "Register","Home")
        </div>
    </div>
    <!-- end wrapper page -->
    <!-- jQuery  -->
    <script src="~/Content/App1/assets/js/jquery.min.js"></script>
    <script src="~/Content/App1/assets/js/popper.min.js"></script>
    <script src="~/Content/App1/assets/js/bootstrap.min.js"></script>
    <script src="~/Content/App1/assets/js/waves.js"></script>
    <script src="~/Content/App1/assets/js/jquery.slimscroll.js"></script>
    <!-- App js -->
    <script src="~/Content/App1/assets/js/jquery.core.js"></script>
    <script src="~/Content/App1/assets/js/jquery.app.js"></script>
    <script>
        $(document).ready(function () {
            function LoginSuccessUser() {

            }
        })
    </script>

</body>
}
</html>
